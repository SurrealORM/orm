{
  "name": "@surrealorm/orm",
  "version": "1.0.0-alpha.5",
  "description": "A TypeScript ORM for SurrealDB with decorators and type safety.",
  "main": "dist/index.cjs",
  "types": "dist/index.d.mts",
  "module": "dist/index.mjs",
  "readme": "README.npm.md",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.npm.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && node -e \"const fs=require('fs'); fs.renameSync('README.md', 'README.github.md'); fs.renameSync('README.npm.md', 'README.md');\"",
    "postpublish": "node -e \"const fs=require('fs'); fs.renameSync('README.md', 'README.npm.md'); fs.renameSync('README.github.md', 'README.md');\""
  },
  "keywords": [
    "surrealdb",
    "orm",
    "typescript",
    "decorators",
    "database"
  ],
  "author": "JustWolf",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustWolfDev/SurrealORM/orm"
  },
  "bugs": {
    "url": "https://github.com/JustWolfDev/SurrealORM/orm/issues"
  },
  "homepage": "https://github.com/JustWolfDev/SurrealORM/orm#readme",
  "dependencies": {
    "reflect-metadata": "^0.1.13",
    "surrealdb": "^1.3.2",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@swc/core": "^1.4.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
