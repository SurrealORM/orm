{
  "name": "surrealorm",
  "version": "0.1.0",
  "description": "A type-safe SurrealDB ORM with decorator schemas",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.mts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "surrealdb",
    "orm",
    "typescript",
    "decorators",
    "database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "surrealdb": "^1.3.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@swc/core": "^1.4.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  }
} 